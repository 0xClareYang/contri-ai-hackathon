# Contri-AI Environment Variables for React
# Copy this file to .env.local for local development

# App Configuration  
REACT_APP_NAME=Contri-AI
REACT_APP_VERSION=2.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_MATRIX_RAIN=true
REACT_APP_ENABLE_ANIMATIONS=true  
REACT_APP_DEBUG_MODE=false

# Performance Settings
REACT_APP_REFRESH_INTERVAL=3000
REACT_APP_MATRIX_RAIN_COUNT=50

# Production URL
REACT_APP_PRODUCTION_URL=https://contri-ai.vercel.app

# Analytics (Optional - for future use)
# REACT_APP_GOOGLE_ANALYTICS=
# REACT_APP_POSTHOG_KEY=

# Social Links
REACT_APP_GITHUB_URL=https://github.com/0xClareYang/contri-ai-hackathon
REACT_APP_AUTHOR=0xClareYang

# Security Settings (Future Use)
# REACT_APP_API_BASE_URL=
# REACT_APP_ENCRYPTION_KEY=

# Note: Only variables prefixed with REACT_APP_ are available in the React app